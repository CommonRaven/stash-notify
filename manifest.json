{
  "name": "Stash Pull Request Notifier",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Stash Pull Request Notifier will let you know when you have pending reviews.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/lib/jquery-2.1.1.min.js",
      "src/lib/lodash.js",
      "src/helpers/badgeHelper.js",
      "src/helpers/jsHelper.js",
      "src/models/stashModel.js",
      "src/bg/responder.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "/src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "omnibox": {
    "keyword": "stash"
  }
}